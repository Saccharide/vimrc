
import matplotlib.pyplot as plt
import numpy as np



def graph(attack_packets, beign_packets):
    maxAttackSize = max(attack_packets)
    maxBeignSize  = max(beign_packets)

    maxIterateSize = max([maxAttackSize, maxBeignSize])

    print("MaxIterateSize = ", maxIterateSize)




graph([1,1,2,3,5,8], [2,2,4,6,6,7,8,9])
